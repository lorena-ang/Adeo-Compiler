int function fib(int x)
{
	if (x == 0) 
    {
		return 0;
	}
    elseif (x == 1)
    {
		return 1;
	}
    else
    {
		return fib(x - 1) + fib(x - 2);
	}
}

int function fact(int x)
{
	if (x == 0)
    {
		return 1;
	}
    else
    {
		return (x * fact(x - 1));
	}
}

main()
{
	var int: x;

	print("Fibonacci series for first 10 numbers: \n");
    for x = 0 to 10 do
	{
		print(fib(x), " ");
	}

	print("\nFactorial of first 10 numbers: \n");
    for x = 1 to 11 do
	{
		print(fact(x), " " );
	}

	print("\n");
}  