var int: i, j, p;
var int: Arreglo[12];
var int: Matriz[12][8];

int function fact(int j)
{
    var int: i;

    i = j + (p - j * 2 + j);

    if (j == 1)
    {
        return (j);
    }
    else
    {
        return (j * fact(j - 1));
    }
}

void function inicia(int y)
{
    var int: x;

    x = 0;

    while (x < 11)
    {
        Arreglo[x] = y * x;
        x = x + 1;
    }
}

main()
{
    read(p);

    j = p * 2;
    i = 0;

    inicia(p * j - 5);

    while(i < 11)
    {
        Arreglo[i] = Arreglo[i] * fact(Arreglo[i] - p);
        Matriz[i][Arreglo[i + p] * p] = Arreglo[i];
        i = i + 1;
    }

    while(i >= 0)
    {
        print("resultado", Arreglo[i], fact(i + 2));
        print(Matriz[i][p * 2 - 1]);
        i = i - 1;
    }
}